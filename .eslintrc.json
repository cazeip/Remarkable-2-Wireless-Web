{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "quotes": [
            "warn",
            "single"
        ],
        "camelcase": "warn",
        "no-unused-vars": "off"
    }
}